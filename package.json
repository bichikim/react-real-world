  "author": "Bichi Kim <bichi@live.co.kr>",
  "main": "packages/web/.next",
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": []
  },
  "scripts": {
    "build": "pnpm run generate --filter @bichi/web && pnpm recursive run build --filter @bichi/web...",
    "dev": "pnpm generate && pnpm recursive run dev-p --no-sort",
    "favicongen": "yarn workspace @bichi/web favicongen",
    "format": "lerna run format --parallel",
    "format:package.json": "sort-package-json \"package.json\" \"packages/*/package.json\" ",
    "generate": "pnpm recursive run generate --filter @bichi/server && pnpm recursive run generate --filter @bichi/web",
    "start": "lerna run start",
    "test": "jest"
  },
  "engines": {
    "node": ">=12",
    "pnpm": ">=5"
  },
  "dependencies": {
    "react": "~17.0.1",
    "react-dom": "~17.0.1"
  },
